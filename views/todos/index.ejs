<%- include('../partials/header') %>
<title><%= title %></title>
<h2>ToDo Lists</h2>
<table>
  <thead>
    <tr>
      <th>List:</th>
      <th>Details:</th>
    </tr>
  </thead>
  <tbody>
    <% todos.forEach(function(l) { %>
    <tr>
      <td><%= l.name %></td>
      <td><a href="/todos/<%= l._id %>">DETAILS</a></td>
    </tr>
    <% }) %>
  </tbody>
</table>



<%- include('../partials/footer') %>